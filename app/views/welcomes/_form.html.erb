<%= simple_form_for@welcome,:html=>{:multipart=>true} do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
     <%= f.input :filename,:as=> :file,:label=>false %>
  </div>

  <div class="form-actions">

  <%= f.button :submit, "Upload",:class=>"btn btn-primary" %>
  </div>
<% end %>
